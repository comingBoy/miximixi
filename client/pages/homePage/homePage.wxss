page {
  height: 100%;
}

.photo {
  background-color: white;
  width: 500rpx;
  height: 700rpx;
  position: absolute;
  border-radius: 25rpx;
  box-shadow: 10rpx 10rpx 10rpx #bbb;
  top: 0;
  left: 0;
  margin: auto;
  right: 0;
  bottom: 0;
}

.album {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
}
